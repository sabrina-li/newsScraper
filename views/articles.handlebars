<ul>
  {{#each articles as |value key|}}
    <li class="box">
        <a href="{{this.link}}" class="title is-4 article-title">{{this.title}}</a>
        <p>{{this.summery}}</p>
        <hr>
        <h6 class="title is-6">Notes:</h6>
        {{#each this.notes}}
          <p >{{this.notes}}<button data-noteID="{{this._id}}" data-articleID="{{value._id}}" class="removeNote">X</button></p>
        {{/each}}
        <form action="/api/addnotes/{{this._id}}" method="post">
            <textarea class="textarea" rows ="1" type="text" name="note"></textarea>
            <input class="button" type="submit" value="Submit">
        </form>
    </li>
  {{/each}}
</ul>